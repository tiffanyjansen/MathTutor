@model IEnumerable<MathCenter.Models.SignIn>

@{
    ViewBag.Title = "Data";
}

<div class="container">
    <div class="center">
        <h1>Here is all of the data from the Math Center sign in</h1>
        <h3>Link to go back: @Html.ActionLink("Go back to home", "Index", "Faculty")</h3>
    </div>
</div>
<br /><br />
<table align="center">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Week)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Hour)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Min)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.VNum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.Classes.FirstOrDefault().CRN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.Classes.FirstOrDefault().DeptPrefix)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.Classes.FirstOrDefault().ClassNum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.Classes.FirstOrDefault().Instructor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.Classes.FirstOrDefault().Days)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.Classes.FirstOrDefault().Time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.Classes.FirstOrDefault().Other)
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Week)
            </td>
            <td>
                @item.Date.ToString("d")
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Hour)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Min)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.VNum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.Classes.FirstOrDefault().CRN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.Classes.FirstOrDefault().DeptPrefix)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.Classes.FirstOrDefault().ClassNum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.Classes.FirstOrDefault().Instructor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.Classes.FirstOrDefault().Days)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.Classes.FirstOrDefault().Time)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.Classes.FirstOrDefault().Other)
            </td>
        </tr>
    }
</table>
