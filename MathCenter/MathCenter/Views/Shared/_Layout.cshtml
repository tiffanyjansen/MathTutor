<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Math Center</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/Icons/open-iconic-master/font/css/open-iconic-bootstrap.css" rel="stylesheet">
</head>
<body>
    <div class="containter-fluid">
        <div class="row">
            <div class="col-12 text-center">
                <img class="logo" src="~/Content/Images/logo_horizontal.gif" />
            </div>
        </div>
        <div class="row">
            <div class="container">
                @RenderBody()
            </div>
        </div>
        <footer>
            <div class="row">
                <div class="col-12 text-center footer">
                    <p>&copy;&nbsp;<img src="~/Content/Images/wou-logo.gif" /></p>
                </div>
            </div>
        </footer>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script type="text/javascript" src="~/Scripts/errors.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>